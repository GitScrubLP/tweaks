[
  // removing default keybinds
  //prettier-ignore
  { "key": "ctrl+l", "command": "-expandLineSelection" },
  { "key": "ctrl+h", "command": "-editor.action.startFindReplaceAction" },
  { "key": "alt+h", "command": "-gitlens.showQuickFileHistory" },
  { "key": "shift+alt+h", "command": "-editor.showCallHierarchy" },
  { "key": "shift+alt+h", "command": "-editor.showIncomingCalls" },
  { "key": "shift+alt+h", "command": "-editor.showOutgoingCalls" },
  { "key": "shift+alt+h", "command": "-references-view.showCallHierarchy" },
  { "key": "shift+alt+h", "command": "-gitlens.showQuickRepoHistory" },
  { "key": "shift+cmd+h", "command": "-workbench.action.replaceInFiles" },
  { "key": "shift+cmd+j", "command": "-workbench.action.search.toggleQueryDetails" },
  { "key": "shift+cmd+k", "command": "-editor.action.deleteLines" },
  { "key": "shift+cmd+l", "command": "-editor.action.selectHighlights" },
  { "key": "shift+cmd+l", "command": "-addCursorsAtSearchResults" },
  { "key": "shift+cmd+l", "command": "-selectAllSearchEditorMatches" },
  { "key": "ctrl+9", "command": "-workbench.action.openEditorAtIndex9" },
  { "key": "cmd+9", "command": "-workbench.action.lastEditorInGroup" },
  { "key": "ctrl+0", "command": "-workbench.action.lastEditorInGroup" },
  { "key": "cmd+0", "command": "-workbench.action.focusSideBar" },
  { "key": "ctrl+`", "command": "-workbench.action.terminal.toggleTerminal" },
  { "key": "cmd+-", "command": "-workbench.action.zoomOut" },
  { "key": "cmd+=", "command": "-workbench.action.zoomIn" },
  { "key": "shift+cmd+u", "command": "-workbench.action.output.toggleOutput" },
  { "key": "cmd+8", "command": "-workbench.action.focusEighthEditorGroup" },
  { "key": "cmd+7", "command": "-workbench.action.focusSeventhEditorGroup" },
  { "key": "shift+cmd+e", "command": "-workbench.view.explorer" },
  { "key": "ctrl+shift+g", "command": "-workbench.view.scm" },
  { "key": "ctrl+alt+n", "command": "-code-runner.run" },
  { "key": "alt+cmd+right", "command": "-workbench.action.nextEditor" },
  { "key": "alt+cmd+left", "command": "-workbench.action.previousEditor" },

  // misc shortcuts for workflow
  {
    "key": "cmd+shift+k",
    "command": "workbench.action.focusActiveEditorGroup",
    "when": "isMac"
  },
  {
    "key": "ctrl+shift+k",
    "command": "workbench.action.focusActiveEditorGroup",
    "when": "isLinux"
  },
  {
    "key": "alt+space",
    "command": "editor.action.triggerSuggest",
    "when": "editorHasCompletionItemProvider && textInputFocus && !editorReadonly"
  },
  {
    "key": "ctrl+alt+n",
    "command": "code-runner.run",
    "when": "isMac"
  },
  {
    "key": "meta+alt+n",
    "command": "code-runner.run",
    "when": "isLinux"
  },
  {
    "key": "ctrl+alt+o",
    "command": "multiCommand.clearTerminal",
    "when": "isMac"
  },
  {
    "key": "meta+alt+o",
    "command": "multiCommand.clearTerminal",
    "when": "isLinux"
  },
  {
    "key": "ctrl+alt+shift+o",
    "command": "workbench.output.action.clearOutput",
    "when": "isMac"
  },
  {
    "key": "meta+alt+shift+o",
    "command": "workbench.output.action.clearOutput",
    "when": "isLinux"
  },
  {
    "key": "shift+cmd+g",
    "command": "git.sync",
    "when": "isMac"
  },
  {
    "key": "shift+ctrl+g",
    "command": "git.sync",
    "when": "isLinux"
  },
  {
    "key": "ctrl+k ctrl+a",
    "command": "editor.action.selectAll",
    "when": "isLinux"
  },

  // MacOS to Linux experience parity
  {
    "key": "ctrl+n",
    "command": "workbench.action.files.newUntitledFile",
    "when": "isLinux",
  },
  {
    "key": "ctrl+o",
    "command": "workbench.action.files.openFile",
    "when": "isLinux"
  },
  {
    "key": "ctrl+f",
    "command": "actions.find",
    "when": "isLinux"
  },
  {
    "key": "ctrl+w",
    "command": "workbench.action.closeActiveEditor",
    "when": "isLinux"
  },

  // tab and editor group navigation vim-style
  // navigate editor groups
  {
    "key": "cmd+l",
    "command": "workbench.action.focusNextGroup",
    "when": "isMac && editorTextFocus && !listFocus"
  },
  {
    "key": "cmd+h",
    "command": "workbench.action.focusPreviousGroup",
    "when": "isMac && editorTextFocus && !inReferenceSearchEditor"
  },
  // navigate editor tabs
  {
    "key": "shift+cmd+l",
    "command": "workbench.action.nextEditor",
    "when": "isMac && editorTextFocus && vim.active"
  },
  {
    "key": "shift+ctrl+l",
    "command": "workbench.action.nextEditor",
    "when": "isLinux && editorTextFocus && vim.active"
  },
  {
    "key": "shift+cmd+h",
    "command": "workbench.action.previousEditor",
    "when": "isMac && editorTextFocus && vim.active"
  },
  {
    "key": "shift+ctrl+h",
    "command": "workbench.action.previousEditor",
    "when": "isLinux && editorTextFocus && vim.active"
  },
  // move editors around
  {
    "key": "cmd+alt+h",
    "command": "workbench.action.moveEditorLeftInGroup",
    "when": "isMac"
  },
  {
    "key": "cmd+alt+l",
    "command": "workbench.action.moveEditorRightInGroup",
    "when": "isMac"
  },
  // moving between groups
  {
    "key": "cmd+alt+left",
    "command": "workbench.action.moveEditorToPreviousGroup",
    "when": "isMac"
  },
  {
    "key": "cmd+alt+right",
    "command": "workbench.action.moveEditorToNextGroup",
    "when": "isMac"
  },
  // moving within group
  {
    "key": "ctrl+shift+alt+j",
    "command": "workbench.action.moveEditorRightInGroup",
    "when": "isMac"
  },
  {
    "key": "ctrl+shift+alt+j",
    "command": "workbench.action.moveEditorRightInGroup",
    "when": "isLinux"
  },
  {
    "key": "ctrl+shift+alt+k",
    "command": "workbench.action.moveEditorLeftInGroup",
    "when": "isMac"
  },
  {
    "key": "ctrl+shift+alt+k",
    "command": "workbench.action.moveEditorLeftInGroup",
    "when": "isLinux"
  },

  // sidebar navigation
  {
    "key": "cmd+b",
    "command": "workbench.action.toggleSidebarVisibility",
    "when": "isMac"
  },
  {
    "key": "ctrl+b",
    "command": "workbench.action.toggleSidebarVisibility",
    "when": "isLinux"
  },
  {
    "key": "ctrl+cmd+shift+alt+b",
    "command": "multiCommand.openFocusSidebar",
    "when": "isMac && !sideBarVisible"
  },
  {
    "key": "ctrl+meta+shift+alt+b",
    "command": "multiCommand.openFocusSidebar",
    "when": "isLinux && !sideBarVisible"
  },
  {
    "key": "ctrl+cmd+shift+alt+b",
    "command": "workbench.action.toggleSidebarVisibility",
    "when": "isMac && sideBarVisible"
  },
  {
    "key": "ctrl+meta+shift+alt+b",
    "command": "workbench.action.toggleSidebarVisibility",
    "when": "isLinux && sideBarVisible"
  },
  {
    "key": "cmd+shift+9",
    "command": "workbench.action.focusSideBar",
    "when": "isMac"
  },
  {
    "key": "ctrl+shift+9",
    "command": "workbench.action.focusSideBar",
    "when": "isLinux"
  },
  {
    "key": "shift+down",
    "command": "workbench.action.nextSideBarView",
    "when": "sideBarFocus"
  },
  {
    "key": "shift+up",
    "command": "workbench.action.previousSideBarView",
    "when": "sideBarFocus"
  },
  {
    "key": "cmd+shift+8",
    "command": "workbench.view.scm",
    "when": "isMac"
  },
  {
    "key": "ctrl+shift+8",
    "command": "workbench.view.scm",
    "when": "isLinux"
  },
  {
    "key": "cmd+shift+7",
    "command": "workbench.view.explorer",
    "when": "isMac"
  },
  {
    "key": "ctrl+shift+7",
    "command": "workbench.view.explorer",
    "when": "isLinux"
  },
  // new file / folder
  {
    "key": ",",
    "command": "explorer.newFile",
    "when": "sideBarFocus && activeViewlet == 'workbench.view.explorer'"
  },
  {
    "key": "shift+,",
    "command": "explorer.newFolder",
    "when": "sideBarFocus && activeViewlet == 'workbench.view.explorer'"
  },
  // focus/close sidebar
  {
    "key": "cmd+h",
    "command": "multiCommand.openFocusSidebar",
    "when": "editorFocus && activeEditorGroupIndex == 1 && !sideBarVisible && !inReferenceSearchEditor"
  },
  {
    "key": "cmd+h",
    "command": "workbench.action.focusSideBar",
    "when": "editorFocus && activeEditorGroupIndex == 1 && sideBarVisible && !inReferenceSearchEditor"
  },
  {
    "key": "cmd+h",
    "command": "workbench.action.toggleSidebarVisibility",
    "when": "sideBarFocus"
  },
  {
    "key": "cmd+left",
    "command": "multiCommand.openFocusSidebar",
    "when": "!sideBarVisible"
  },
  {
    "key": "cmd+left",
    "command": "workbench.action.focusSideBar",
  },
  // focus back to editor
  {
    "key": "cmd+up",
    "command": "workbench.action.focusActiveEditorGroup",
  },
  {
    "key": "cmd+right",
    "command": "workbench.action.focusActiveEditorGroup",
  },
  {
    "key": "cmd+l",
    "command": "workbench.action.focusActiveEditorGroup",
    "when": "sideBarFocus"
  },


  // panel (bottom part) navigation
  {
    "key": "cmd+j",
    "command": "workbench.action.togglePanel",
    "when": "isMac"
  },
  {
    "key": "ctrl+j",
    "command": "workbench.action.togglePanel",
    "when": "isLinux"
  },
  {
    "key": "cmd+down",
    "command": "workbench.action.focusPanel",
  },
  {
    "key": "cmd+shift+j",
    "command": "workbench.action.focusPanel",
    "when": "isMac"
  },
  {
    "key": "ctrl+shift+j",
    "command": "workbench.action.focusPanel",
    "when": "isLinux"
  },
  {
    "key": "shift+right",
    "command": "workbench.action.nextPanelView",
    "when": "panelFocus"
  },
  {
    "key": "shift+left",
    "command": "workbench.action.previousPanelView",
    "when": "panelFocus"
  },
  {
    "key": "cmd+=",
    "command": "workbench.action.terminal.toggleTerminal",
    "when": "isMac"
  },
  {
    "key": "ctrl+=",
    "command": "workbench.action.terminal.toggleTerminal",
    "when": "isLinux"
  },
  {
    "key": "cmd+-",
    "command": "workbench.action.output.toggleOutput",
    "when": "isMac"
  },
  {
    "key": "ctrl+-",
    "command": "workbench.action.output.toggleOutput",
    "when": "isLinux"
  },


  // allowing easier command access for common vim command actions
  // (also helps match my linux keybindings / muscle memory)
  {
    "key": "cmd+e",
    "command": "extension.vim_ctrl+e",
    "when": "isMac && editorTextFocus && vim.active && vim.mode != 'Insert'"
  },
  {
    "key": "cmd+y",
    "command": "extension.vim_ctrl+y",
    "when": "isMac && editorTextFocus && vim.active && vim.mode != 'Insert'"
  },
  {
    "key": "cmd+u",
    "command": "extension.vim_ctrl+u",
    "when": "isMac && editorTextFocus && vim.active && vim.mode != 'Insert'"
  },
  {
    "key": "cmd+d",
    "command": "extension.vim_ctrl+d",
    "when": "isMac && editorTextFocus && vim.active && vim.mode != 'Insert'"
  },
  {
    "key": "cmd+r",
    "command": "extension.vim_ctrl+r",
    "when": "isMac && editorTextFocus && vim.active && vim.mode != 'Insert'"
  },

  // vim-style vertical navigation in suggestion/prompt popups
  // intellisense suggestions
  {
    "key": "alt+j",
    "command": "selectNextSuggestion",
    "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus"
  },
  {
    "key": "alt+k",
    "command": "selectPrevSuggestion",
    "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus"
  },

  // peek widget navigation
  {
    "key": "left",
    "command": "togglePeekWidgetFocus",
    "when": "!inReferenceSearchEditor && referenceSearchVisible && listFocus"
  },
  {
    "key": "space",
    "command": "togglePeekWidgetFocus",
    "when": "vim.mode != 'Insert' && referenceSearchVisible && !listFocus"
  },
  {
    "key": "space",
    "command": "workbench.action.focusActiveEditorGroup",
    "when": "vim.mode != 'Insert' && inReferenceSearchEditor"
  },
  {
    "key": "cmd+l",
    "command": "togglePeekWidgetFocus",
    "when": "inReferenceSearchEditor"
  },
  {
    "key": "cmd+l",
    "command": "togglePeekWidgetFocus",
    "when": "!inReferenceSearchEditor && referenceSearchVisible && listFocus"
  },
  {
    "key": "cmd+h",
    "command": "togglePeekWidgetFocus",
    "when": "inReferenceSearchEditor"
  },
  {
    "key": "cmd+h",
    "command": "togglePeekWidgetFocus",
    "when": "!inReferenceSearchEditor && referenceSearchVisible && listFocus"
  },
  {
    "key": "h",
    "command": "togglePeekWidgetFocus",
    "when": "!inReferenceSearchEditor && referenceSearchVisible && listFocus"
  },
]
